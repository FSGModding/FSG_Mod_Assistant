{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["./modAssist_main.js", "./lib"]
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash/",
        "encoding": "utf8",
        "destination": "jsdoc_server/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}