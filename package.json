{
  "name": "fsg-mod-assistant",
  "version": "3.9.72",
  "description": "FSG Farm Sim Mod Assistant",
  "main": "modAssist_main.js",
  "homepage": "https://github.com/FSGModding/FSG_Mod_Assistant#readme",
  "keywords": [],
  "author": {
    "name": "FSG Modding",
    "email": "jtsage+datebox@gmail.com",
    "url": "https://www.fsgmodding.com/"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FSGModding/FSG_Mod_Assistant/issues/"
  },
  "repository": "github:FSGModding/FSG_Mod_Assistant",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "node ./test/test.js",
    "depends": "node ./test/outdated-deps.js",
    "generate-docs": "node_modules\\.bin\\jsdoc -c .jsdoc.json"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/eslint-parser": "^7.24.1",
    "ansi-colors": "^4.1.3",
    "clean-css": "^5.3.3",
    "deepl-node": "^1.12.0",
    "docdash": "^2.0.2",
    "electron": "^29.1.5",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "html-validate": "^8.18.0",
    "jsdoc": "^4.0.2"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "archiver": "^7.0.1",
    "discord-rpc": "^4.0.1",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.2.1",
    "fast-xml-parser": "^4.3.6",
    "glob": "^10.3.10",
    "node-stream-zip": "^1.15.0",
    "unzip-stream": "^0.3.1",
    "wasm-vips": "^0.0.8"
  },
  "build": {
    "appId": "jtsage.fsmodassist",
    "productName": "FSModAssistant",
    "files": [
      "!README.*",
      "!test/*",
      "!screen_shots/**",
      "!**/.vscode/*",
      "!.github/*",
      "!/dist/*",
      "!/docs/*",
      "!/img_src/*",
      "!/renderer/inc/src/*"
    ],
    "extraFiles": [
      "texconv.exe"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "portable": {
      "artifactName": "${productName} Portable ${version}.exe"
    }
  }
}
